<h3><%= @cuisine.name %></h3>
<%= form_with model: [@restaurant , @cuisine] do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class:"form-central" %>
  </div>
  <div class="form-group">
    <%= form.label :restaurant_id %>
    <%= form.text_area :restaurant_id , class:"form-central"%>
  </div>
 
  <div>
    <%= form.submit "Update Cuisine" %>
  </div>
<% end %>
